# Gemini API Key (exposed to the client)
NEXT_PUBLIC_GEMINI_API_KEY=

# Sepolia Testnet RPC URL (for Ethereum and ENS)
# Get one from Infura, Alchemy, etc.
SEPOLIA_RPC_URL=

# Arbitrum Sepolia Testnet RPC URL (for PYUSD transfers)
# Default: https://sepolia-rollup.arbitrum.io/rpc
ARBITRUM_SEPOLIA_RPC_URL=https://arbitrum-sepolia.drpc.org

# Private key for the Ethereum wallet (must have Arbitrum Sepolia ETH for gas and the PYUSD token)
# IMPORTANT: Do NOT include the "0x" prefix
ETH_PRIVATE_KEY=

# Flow Testnet Configuration
# Get a testnet account from the Flow Faucet: https://testnet-faucet.onflow.org/
FLOW_ACCESS_NODE="https://rest-testnet.onflow.org"
FLOW_ACCOUNT_ADDRESS=
FLOW_PRIVATE_KEY=
